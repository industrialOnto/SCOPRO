#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/indonto/ontologies/2014/0/SCOPRO#Actualization
:Actualization rdf:type owl:Class ;
               rdfs:subClassOf :Information_RR ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :isInvolvedIn ;
                                 owl:allValuesFrom :Modification
                               ] ;
               rdfs:comment "Rol de la información que es requerida para la ejecución de un proceso, o alguna parte del mismo, y que como resultado de la ejecución sufre alguna modificación" .


###  http://www.semanticweb.org/indonto/ontologies/2014/0/SCOPRO#Atomic_Task
:Atomic_Task rdf:type owl:Class ;
             rdfs:subClassOf :Task ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :isComprisedOfSubProcess ;
                               owl:qualifiedCardinality "0"^^xsd:nonNegativeInteger ;
                               owl:onClass :Process
                             ] ;
             owl:disjointWith :Composite_Task ;
             rdfs:comment "Tarea que no se descompone en otras tareas" .


###  http://www.semanticweb.org/indonto/ontologies/2014/0/SCOPRO#Atomic_Temporal_Relationship
:Atomic_Temporal_Relationship rdf:type owl:Class ;
                              rdfs:subClassOf :Temporal_Relationship ,
                                              [ rdf:type owl:Class ;
                                                owl:unionOf ( [ owl:intersectionOf ( [ rdf:type owl:Class ;
                                                                                       owl:complementOf [ rdf:type owl:Restriction ;
                                                                                                          owl:onProperty :restrains ;
                                                                                                          owl:someValuesFrom :Process
                                                                                                        ]
                                                                                     ]
                                                                                     [ rdf:type owl:Class ;
                                                                                       owl:complementOf [ rdf:type owl:Restriction ;
                                                                                                          owl:onProperty :withRespectTo ;
                                                                                                          owl:someValuesFrom :Process
                                                                                                        ]
                                                                                     ]
                                                                                   ) ;
                                                                rdf:type owl:Class
                                                              ]
                                                              [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                                       owl:onProperty :restrains ;
                                                                                       owl:someValuesFrom :Process
                                                                                     ]
                                                                                     [ rdf:type owl:Restriction ;
                                                                                       owl:onProperty :withRespectTo ;
                                                                                       owl:someValuesFrom :Process
                                                                                     ]
                                                                                   ) ;
                                                                rdf:type owl:Class
                                                              ]
                                                            )
                                              ] ,
                                              [ rdf:type owl:Class ;
                                                owl:complementOf [ owl:intersectionOf ( [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                                                                 owl:onProperty :restrains ;
                                                                                                                 owl:someValuesFrom :Process
                                                                                                               ]
                                                                                                               [ rdf:type owl:Restriction ;
                                                                                                                 owl:onProperty :withRespectTo ;
                                                                                                                 owl:someValuesFrom :Process
                                                                                                               ]
                                                                                                             ) ;
                                                                                          rdf:type owl:Class
                                                                                        ]
                                                                                        [ rdf:type owl:Restriction ;
                                                                                          owl:onProperty :isPartOfTempRelat ;
                                                                                          owl:someValuesFrom :Composite_Temporal_Relationship
                                                                                        ]
                                                                                      ) ;
                                                                   rdf:type owl:Class
                                                                 ]
                                              ] ;
                              owl:disjointWith :Composite_Temporal_Relationship ;
                              rdfs:comment "Relación temporal entre dos procesos mediante la cual el espacio de tiempo de ejecución de uno de ellos se restringe con respecto a la realización del otro. Una relación temporal atómica no está compuesta por otras relaciones temporales" .


###  http://www.semanticweb.org/indonto/ontologies/2014/0/SCOPRO#Before
:Before rdf:type owl:Class ;
        rdfs:subClassOf :Atomic_Temporal_Relationship ;
        owl:disjointWith :During ,
                         :Equal ,
                         :Meets ,
                         :Overlaps ;
